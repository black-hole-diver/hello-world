/*for loop exercise to find the sum of the number from 50 to 100
the counterpart of exercise 1.19*/

#include <iostream>
using namespace std; //to not write std:: every time I write code

int main()
{
	int sum=0;
  	for (int i= 50; i <= 100; ++i)
    	sum += i;
    cout <<sum<< endl;
    return 1;
}
