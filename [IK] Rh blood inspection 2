#include <iostream>
using namespace std;

int main()
{
	char first,second;
	cout <<"Give me your chromosome: \n"<< endl;
	cin >>first;
	cout <<"Give me your 2nd chromosome: \n"<< endl;
	cin >>second;
	if (first == '+' or second == '+'){
		cout <<"Rh+" << endl;
	}
	else if (first == '-' and second == '-'){
		cout <<"Rh-" << endl;
	}
	else{
		cout <<"Write + or -!" <<endl;
	}
	
	return 0;
}
